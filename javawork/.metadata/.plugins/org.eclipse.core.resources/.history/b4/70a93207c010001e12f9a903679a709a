package day0622;

import java.util.Scanner;

public class ScanGugu_05 {

	public static void main(String[] args) {
		// 단을 입력해서 해당단 출력하기_Scanner
		//0을 입력시 종료
		Scanner sc=new Scanner(System.in);
		int dan, j=1;
		
		/*for(j=1;j<=9;j++)
			System.out.println(dan+"*"+j+"="+dan*j);*/
		while(true)
			{
			System.out.println("단 입력");
			dan=sc.nextInt();
				if(dan==0)
					break;
				for(j=1;j<=9;j++)
				System.out.println(dan+"*"+j+"="+dan*j);
				j++;
			}

	}

}
