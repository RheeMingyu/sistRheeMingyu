package day0616;

public class ParseIntArgs_03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*[결과출력]
		 * 내이름은 김영환
		 * 자바점수는 88
		 * 오라클점수는 77
		 * 두과목의 합계는 165점 입니다*/
		
		int num1=args[0];
		int num2=args[1];
		
		System.out.println("[결과출력]");
		System.out.println("자바점수는 "+args[0]);
		System.out.println("오라클점수는 "+args[1]);
		System.out.println("두과목의 합계는"+(args[0]+args[1])+"입니다");

	}

}
